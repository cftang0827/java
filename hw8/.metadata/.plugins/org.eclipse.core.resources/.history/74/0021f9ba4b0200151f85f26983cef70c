
public class Triangle extends Shape {
	public Triangle(double length) {
		super(length);
	}

	public void setLength(double length){
		this.length = length;
	}

	@Override
	public double getArea() {
		double a = (length * Math.sqrt(3)* length)/2;
		return a;
	}

	@Override
	public double getPerimeter() {
		return 3*length;
	}
}
